{
  "name": "NuGetFeed",
  "version" : "1.0.1",
  "description" : "Follow your favorite NuGet packages as RSS feeds.",
  "content_scripts": [
    {
      "matches": ["http://nuget.org/List/Packages/*","http://www.nuget.org/List/Packages/*"],
      "js": ["NuGetFeed.js"],
      "run_at": "document_end"
    }
  ],
  "permissions" : [
    "http://*/*"
   ],
  "minimum_chrome_version" : "6.0.0.0",
  "icons" : {
    "16": "nugetfeed-16.png",
    "48": "nugetfeed-48.png",
    "128": "nugetfeed-128.png"
  }
}